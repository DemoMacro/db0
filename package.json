{
  "name": "sql0",
  "version": "0.1.0",
  "description": "",
  "repository": "unjs/sql0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./connectors/*": {
      "types": "./connectors/*.d.ts",
      "import": "./connectors/*.mjs",
      "require": "./connectors/*.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "connectors"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts . && prettier -c src test",
    "lint:fix": "eslint --ext .ts . --fix && prettier -w src testpg",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release --push && pnpm publish",
    "test": "pnpm lint && vitest run --coverage",
    "sql0": "pnpm jiti src/cli"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "@types/pg": "^8.6.6",
    "@vitest/coverage-c8": "^0.31.1",
    "better-sqlite3": "^8.3.0",
    "changelogen": "^0.5.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-unjs": "^0.1.0",
    "jiti": "^1.18.2",
    "pg": "^8.11.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.1"
  },
  "peerDependencies": {
    "better-sqlite3": "^8.3.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.5.1"
}
